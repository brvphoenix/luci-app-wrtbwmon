{
	"luci-app-wrtbwmon": {
		"description": "Grant access to LuCI app wrtbwmon",
		"read": {
			"file": {
				"/tmp/usage.db": [ "read" ],
				"/etc/wrtbwmon.user": [ "read" ],
				"/usr/sbin/wrtbwmon": [ "exec" ],
				"/bin/cp": [ "exec" ]
			},
			"uci": [ "wrtbwmon" ]
		},
		"write": {
			"file": {
				"/bin/cat": [ "exec" ],
				"/bin/rm": [ "exec" ],
				"/bin/cp": [ "exec" ],
				"/etc/wrtbwmon.user": [ "write" ]
			},
			"uci": [ "wrtbwmon" ]
		}
	}
}
